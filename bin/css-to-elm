#!/usr/bin/env node

const fs = require('fs')
const cssToElm = require('../index').default

fs.readFile('app.css', (err, css) => {
  cssToElm(css)
        .then(result => {
          file = "import Css exposing (..)\n"
          file += "import Css.Elements exposing (..)\n\n"
          file += "css = \n "
          file += result
          fs.writeFile('gen.elm', file)
        })
})